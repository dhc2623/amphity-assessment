#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/SIPL-Projects/Amphity-Assessment/assessment-fullstack-vuetssql-main/assessment-fullstack-vuetssql-main/node_modules/.pnpm/tailwindcss@3.4.0/node_modules/tailwindcss/lib/node_modules:/mnt/d/SIPL-Projects/Amphity-Assessment/assessment-fullstack-vuetssql-main/assessment-fullstack-vuetssql-main/node_modules/.pnpm/tailwindcss@3.4.0/node_modules/tailwindcss/node_modules:/mnt/d/SIPL-Projects/Amphity-Assessment/assessment-fullstack-vuetssql-main/assessment-fullstack-vuetssql-main/node_modules/.pnpm/tailwindcss@3.4.0/node_modules:/mnt/d/SIPL-Projects/Amphity-Assessment/assessment-fullstack-vuetssql-main/assessment-fullstack-vuetssql-main/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/SIPL-Projects/Amphity-Assessment/assessment-fullstack-vuetssql-main/assessment-fullstack-vuetssql-main/node_modules/.pnpm/tailwindcss@3.4.0/node_modules/tailwindcss/lib/node_modules:/mnt/d/SIPL-Projects/Amphity-Assessment/assessment-fullstack-vuetssql-main/assessment-fullstack-vuetssql-main/node_modules/.pnpm/tailwindcss@3.4.0/node_modules/tailwindcss/node_modules:/mnt/d/SIPL-Projects/Amphity-Assessment/assessment-fullstack-vuetssql-main/assessment-fullstack-vuetssql-main/node_modules/.pnpm/tailwindcss@3.4.0/node_modules:/mnt/d/SIPL-Projects/Amphity-Assessment/assessment-fullstack-vuetssql-main/assessment-fullstack-vuetssql-main/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/tailwindcss@3.4.0/node_modules/tailwindcss/lib/cli.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/tailwindcss@3.4.0/node_modules/tailwindcss/lib/cli.js" "$@"
fi
